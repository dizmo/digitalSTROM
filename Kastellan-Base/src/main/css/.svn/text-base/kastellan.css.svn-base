input {
	-apple-dashboard-region: dashboard-region(control rectangle 0px 0px 0px 0px);
}


a,
a:hover,
a:active,
a:visited {
	color:white;
	text-decoration:underline;
}
.top {
	-webkit-border-image: url('images/top.png') 5 10 5 10 stretch stretch;
	border-width: 5px 10px 5px 10px;
	position:absolute;
	left:0px;
	top:0px;
	width:100%;
	height:0px;
	z-index:200;
}
.bottom {
	-webkit-border-image: url('images/bottom.png') 5 10 5 10 stretch stretch;
	border-width: 5px 10px 5px 10px;
	position:absolute;
	left:0px;
	bottom:0px;
	width:100%;
	height:0px;
	z-index:200;
}

.backside_container .okTextbutton {
	-webkit-border-image: url('images/button_general.png') 6 7 6 7 stretch stretch;
	border-width: 6px 7px 6px 7px;
	background-color:transparent;
	color:white;
	font-size: 10px;
	padding: 2px 5px 2px 5px;
}
.backside_container .okTextbutton:active {
	-webkit-border-image: url('images/button_general_active.png') 6 7 6 7 stretch stretch;
}

.front button {
	-webkit-border-image: url('images/button_general.png') 6 7 6 7 stretch stretch;
	border-width: 6px 7px 6px 7px;
	background-color:transparent;
	color:white;
	font-size: 10px;
	padding: 2px 3px 2px 3px;
}
.front button.current {
	-webkit-border-image: url('images/button_general_active.png') 6 7 6 7 stretch stretch;
}

.front .toolbar {
	background-color: #434343;
	height: 39px;
	padding-top: 3px;
	background-image:url('images/toolbarbottom.png');
	background-repeat:no-repeat;
	background-position:0px 32px;
	padding-left:3px;
}

body {
	font-family:HelveticaNeue,Helvetica;
	margin:0px;
}
.space_after {
	margin-bottom:10px;
}

li button {
	display:inline;
}
li span.editable form {
	display:inline;
}
li .editable {
	color:white;
	font-size:12px;
	margin-top:7px;
	margin-left:10px;
	float:left;
}
li.editing .editable {
	margin-top:5px;
	margin-left:4px;
}
li .editable button.submit {
	background-image:url('images/done.png');
	float:right;
	margin-right:4px;
	width:15px;
	height:15px;
	border-width:0px;
	background-color:transparent;
}
li .editable button.cancel {
	background-image:url('images/cancel.png');
	width:15px;
	float:right;
	margin-right:4px;
	margin-left:4px;
	height:15px;
	border-width:0px;
	background-color:transparent;
}
li button.delete {
	background-image:url('images/delete.png');
	width:15px;
	float: left;
	margin-right:4px;
	margin-left:4px;
	height:15px;
	border-width:0px;
	background-size: 100%;
	background-color:transparent;
	margin-left: 7px;
	margin-top: 5px;
}
input {
	-webkit-border-image: url('images/backInput.png') 4 4 4 4  stretch stretch;
	border-width:4px;
	background:transparent;
	color:white;
	font-size: 12px;
}
li .editable input {
	position:relative;
	top:-3px;
}

.activities_back li .editable input {
	width:100px !important;
	height:11px !important;
}
.genesis_back  li .editable input {
	height:11px !important;
}

.infobox {
	-webkit-border-image: url('images/infobox.png') 3 3 3 20  stretch stretch;
	border-width:3px 3px 3px 20px;
	background-color:transparent;
	height:16px;
	line-height: 13px;
	color: white;
	font-size: 10px;
	padding-left:5px;
}
.infobox2 {
	-webkit-border-image: url('images/infobox.png') 3 3 3 20  stretch stretch;
	border-width:3px 3px 3px 20px;
	background-color:transparent;
	height:34px;
	line-height: 15px;
	color: white;
	font-size: 10px;
	padding-left:5px;
}
.infobox.problem {
	-webkit-border-image: url('images/infobox_error.png') 3 3 3 20  stretch stretch;
	background-color:transparent;
}
.infobox2.problem {
	-webkit-border-image: url('images/infobox_error_2.png') 3 3 3 20  stretch stretch;
	background-color:transparent;
}

.backside_container {
	background-color:#323232;
	position:absolute;
	left:0px;
	top:0px;
