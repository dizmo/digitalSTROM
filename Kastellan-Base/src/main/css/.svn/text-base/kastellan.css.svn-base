/* finish button on backside of a widget */
.backside_container .finish-btn {
	-webkit-border-image: url('images/button_done.png') 8 8 8 8 stretch stretch;
	border-width: 8px;
	margin: 0px;
	padding: 0px;
	background-color:rgba(255,0,0,0);
	bottom: 20px;
	position: absolute;
	right: 20px;
	font-family:HelveticaNeue,Helvetica;
	font-weight:normal;
	color:#FFFFFF;
	font-size:14px;
	line-height: 14px;
	height: 36px;
	padding-left: 6px;
	padding-right: 6px;
	cursor: pointer;
}
.backside_container .finish-btn:hover,
.backside_container .finish-btn:active{
	-webkit-border-image: url('images/button_done_active.png') 8 8 8 8 stretch stretch;
}

/* pulldown menu 1*/
.select_trigger {
	-webkit-border-image: url('images/pulldown_trigger.png') 8 28 8 7 stretch stretch;	
	border-left:7px;
	border-top:8px;
	border-bottom:8px;
	border-right:28px;
	height: 14px;
	padding-top: 1px;
	color: white;
	font-family:HelveticaNeue,Helvetica;
	font-size: 12px;
	cursor:pointer;
	padding-left:7px;
	-apple-dashboard-region: dashboard-region(control rectangle 0px 0px 0px 0px);

}
ul.select {
	padding:0px;
	margin:0px;
}
ul.select li {
	list-style: none;
	-webkit-border-image: url('images/pulldown_default.png') 5 5 5 5 stretch stretch;
	border-left:5px;
	border-top:5px;
	border-bottom:5px;
	border-right:5px;
	height:16px;
	color: white;
	font-family:HelveticaNeue,Helvetica;
	font-size: 12px;
	cursor:pointer;
	padding-left:3px;
}
.scrolling_container {
	-webkit-border-image: url('images/scrolling.png') 15 15 22 11 stretch stretch;
	border-left:11px;
	border-top:15px;
	border-bottom:22px;
	border-right:15px;
	margin-left: -4px;
	margin-top: -4px;
	z-index:100;
	/*overflow:hidden;*/
}
.scrolling {
	margin-left: -4px;
	margin-top: -3px;
}
.scrolling > ul.select {
	margin-left:-3px;
}
.select_scrollbar_container {
	position: relative;
	z-index:101;
}
.select_scrollbar_container .ui-slider {
