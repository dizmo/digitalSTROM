//= compat
//= require <flab/exception/Builder>
//= require <flab/exception/ExceptionBase>

/**
 * Base class for all kind of data errors, including Ajax load and
 * store failures, parse errors, unexpected server responses, or
 * wrong data format.
 *
 * @class flab.exception.DataError
 */
flab.exception.DataError = flab.exception.builder(
    'DataError',
    flab.exception.ExceptionBase
);
